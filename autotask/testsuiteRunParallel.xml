<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestSuiteChrome" parallel="classes" >
  <test name="Testwithchrome">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="parallel.TestRegistration"/>
      <class name="parallel.TestSearchByKeyword"/>
      <class name="parallel.TestPras"/>
    </classes>
  </test> <!-- Testwithchrome -->
  
  <test name="Testwithfirefox">
  <parameter name="browser" value="firefox"></parameter>
    <classes>
      <class name="parallel.TestRegistration"/>
      <class name="parallel.TestSearchByKeyword"/>
      <class name="parallel.TestPras"/>
    </classes>
  </test> <!-- Testwithfirefox -->
  
</suite> <!-- TestSuiteChrome -->
